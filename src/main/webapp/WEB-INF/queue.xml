<queue-entries>
  <queue>
    <name>default</name>
    <rate>5/s</rate>
    <bucket-size>5</bucket-size>
  </queue>
  <queue>
    <name>notification</name>
    <rate>1/s</rate>
    <retry-parameters>
      <task-retry-limit>7</task-retry-limit>
      <task-age-limit>2d</task-age-limit>
    </retry-parameters>
  </queue>
</queue-entries>